Edit The Game:

<%= form_for :game, method: "PATCH" do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %><br />
  <%= f.label :home %>:
  <%= f.text_field :home %><br />
  <%= f.label :away %>:
  <%= f.text_field :away %><br />
  <%= f.submit %>
<% end %>

<%=form_for @game, url: delete_game_path, method: :delete do |f| %>
  <%=f.submit "Delete Game!" %>
<%end%>