<%=form_for :question, url: create_question_path do |f| %>
  <li><%= f.label :body %>
  <%=f.text_field :body %></li>
  <li><%=f.label 'First Choice:' %>
  <%=f.text_field :choice1 %></li>
  <li><%=f.label 'Second Choice:' %>
  <%=f.text_field :choice2 %></li>
  <%=hidden_field(:question, :prop_id, :value => params[:id])%>
  <%= f.submit 'Create Prop Bet'%>
<%end%>
<%= link_to 'Done Adding Questions', show_prop_bet_path(@prop_bet) %>